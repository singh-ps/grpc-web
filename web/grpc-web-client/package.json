{
  "name": "grpc-web-client",
  "version": "0.1.0",
  "dependencies": {
    "google-protobuf": "^3.21.4",
    "grpc-web": "^1.5.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "npm run proto-regen && tsc && vite build",
    "start": "npm run build && vite --host",
    "proto-gen": "./proto.sh",
    "clean": "rm -rf dist src/generated",
    "proto-regen": "npm run clean && npm run proto-gen"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-commonjs": "^0.10.4"
  }
}
